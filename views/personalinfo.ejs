    <div class="container">
      
        <div class="CTA">
          <h3>Re-Estate</h3>
        </div>
      

      <div class="leftbox">
        <nav>
          <a href="#" class="active">
            <i class="fa fa-user"></i>
          </a>
          <a href="/myproducts">
            
            <i class="fa fa-tasks"></i>
          </a>
          <a href="/wishlist">
            <i class="fa fa-tv"></i>
          </a>
          
          <a href="#">
            <i class="fa fa-cog"></i>
          </a>
        </nav>
      </div>

      <div class="rightbox">
        <div class="profile">
          <h1>Personal Info</h1>
          <form id="myForm" action="/personalinfo" method="post">
            <h2>UserName</h2>
            <input type="text" id="username" value="" value="Initial value" readonly>
            <div id='nameErr' class='err'></div>
            <i class="error-icon fas fa-exclamation-circle" id="er1"></i>
            <br>
            <br>
            <h2>Email</h2>
            <input type="text" id="email" value="" value="Initial value" readonly>
            <input type="button" value="Edit Email" class="changeuser" onclick="enableEditing(this)">
            <div id='emailErr' class='err'></div>
            <i class="error-icon fas fa-exclamation-circle" id="er2"></i>
            <br>
            <br>
            <h2>Phone</h2>
            <input type="text" id="phone" value="" value="Initial value" readonly>
            <input type="button" value="Edit Phone" class="changeuser" onclick="enableEditing(this)">
            <div id='phoneerr' class='err'></div>
            <i class="error-icon fas fa-exclamation-circle" id="er3"></i>
            <br>
            <button id="changebutton" type="submit">Submit Changes</button>
            <a href="/myproducts" class="mypost">My Posts</a> 
            
            <h1 id="donechange">Data Changed</h1>
          </form>
        </div>
      </div>
